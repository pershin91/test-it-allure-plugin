<idea-plugin>
    <id>com.github.clientcyc.test-it-idea-plugin</id>
    <name>Test-It Autotest Annotations Custom ClientCycle</name>
    <vendor/>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <projectConfigurable
                groupId="tools"
                displayName="Test-It Plugin Custom ClientCycle"
                id="TestIt.ClientCycle.Preferences.PluginConfigurable"
                instance="com.github.clientcyc.testit.plugin.settings.PluginConfigurable"/>
        <projectService serviceImplementation="com.github.clientcyc.testit.plugin.settings.PluginSettings"/>
    </extensions>

    <actions>
        <action id="TestIt.ClientCycle.Create.Autotest.Action"
                class="com.github.clientcyc.testit.plugin.actions.AddTestItAutoTestAnnotationsAction"
                text="Test-It Autotest Annotations Custom ClientCycle">
            <add-to-group group-id="GenerateGroup" anchor="first"/>
        </action>
    </actions>
</idea-plugin>
