<idea-plugin>
    <id>com.github.clientcyc.clientcyc-test-it-idea-plugin</id>
    <name>Clientcyc Test-It Allure Annotations</name>
    <vendor/>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <projectConfigurable
                groupId="tools"
                displayName="Clientcyc Test-It Plugin"
                id="preferences.Clientcyc.PluginConfigurable"
                instance="com.github.clientcyc.testit.plugin.settings.PluginConfigurable"/>
        <projectService serviceImplementation="com.github.clientcyc.testit.plugin.settings.PluginSettings"/>
    </extensions>

    <actions>
        <action id="TestIt.Clientcyc.Create.Autotest.Action"
                class="com.github.clientcyc.testit.plugin.actions.AddTestItAutoTestAnnotationsAction"
                text="Clientcyc Test-It Autotest Annotations">
            <add-to-group group-id="GenerateGroup" anchor="first"/>
        </action>
    </actions>
</idea-plugin>
